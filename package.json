{
  "name": "pos",
  "description": "pos",
  "version": "0.0.6",
  "license": "MIT",
  "author": "mighty <mightyinsomniak@gmail.com> (http://webcreators.be/)",
  "main": "src/electron.js",
  "scripts": {
    "dev": "concurrently \"cross-env NODE_ENV=development sapper dev\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development electron .\"",
    "build": "sapper build",
    "releasewin": "cmd /c release.sh",
    "export": "sapper export --legacy",
    "start": "concurrently \"cross-env BROWSER=none NODE_ENV=development node __sapper__/build\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development electron .\"",
    "startprod": "electron .",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "package": "yarn build && electron-packager . pos --overwrite --all --icon=static/icons/win/icon.ico --prune=true --out=dist --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"POS application\"",
    "setup": "electron-installer-windows --src dist/pos-win32-x64/ --dest dist/installers/"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "electron-squirrel-startup": "^1.0.0",
    "helmet": "^3.18.0",
    "polka": "^0.5.0",
    "sirv": "^0.4.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "electron": "5.0.1",
    "electron-installer-windows": "^1.1.1",
    "electron-winstaller": "^3.0.4",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.0.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "10.0.0",
    "rollup-plugin-node-resolve": "5.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "sapper": "^0.27.0",
    "svelte": "^3.0.0",
    "wait-on": "^3.2.0"
  }
}
